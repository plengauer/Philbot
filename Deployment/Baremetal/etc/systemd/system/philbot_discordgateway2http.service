[Unit]
Description=""
[Service]
Type=simple
ExitType=main
Restart=always
RestartSec=1
StartLimitBurst=10
User=root
WorkingDirectory=/opt/philbot
ExecStart=/bin/bash /usr/bin/genericstart.sh /etc/philbot-baremetal/environment.properties.discordgateway2http /opt/philbot node.js philbot-discordgateway2http
[Install]
WantedBy=multi-user.target
