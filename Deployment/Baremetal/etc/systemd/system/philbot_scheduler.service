[Unit]
Description=""
[Service]
Type=simple
ExitType=main
Restart=always
RestartSec=1
StartLimitBurst=10
User=root
WorkingDirectory=/opt/philbot
ExecStart=bash /usr/local/sbin/genericstart.sh /etc/philbot-baremetal/environment.properties.scheduler /opt/philbot ruby philbot-scheduler
[Install]
WantedBy=multi-user.target
