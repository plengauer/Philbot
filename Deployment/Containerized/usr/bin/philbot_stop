#!/bin/bash
source /usr/bin/philbot_init_otel
source /opt/philbot/shards
seq -s " philbot-discordgateway2http_" 0 $(($SHARD_COUNT-1)) | xargs -I '{}' echo philbot-discordgateway2http_'{}' | xargs -I {IMAGES} parallel -i docker rm {} --force -- philbot-scheduler philbot-discordgateway2httpmaster {IMAGES} philbot-backend philbot-voice philbot-selfmonitoring
exit 0
