#!/bin/bash
source /usr/bin/philbot_init_otel
source /opt/philbot/shards
shard_container_names=$(echo philbot-discordgateway2http_$(seq -s " philbot-discordgateway2http_" 0 $(($SHARD_COUNT-1))))
docker stop philbot-voice philbot-backend $shard_container_names philbot-discordgateway2httpmaster philbot-scheduler
docker rm philbot-voice philbot-backend $shard_container_names philbot-discordgateway2httpmaster philbot-scheduler
exit 0
