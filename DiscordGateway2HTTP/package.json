    "@opentelemetry/exporter-metrics-otlp-proto": "0.50.0",
    "opentelemetry-resource-detector-git": "0.29.0",
    "@opentelemetry/resource-detector-github": "0.28.1",
    "@opentelemetry/resource-detector-container": "0.3.8",
    "@opentelemetry/resource-detector-aws": "1.4.1",
    "@opentelemetry/resource-detector-gcp": "0.29.8",
    "xhr2": "0.2.1"
  },
  "scripts": {
    "start": "node discordgateway2http.js",
    "exec-start": "exec node discordgateway2http.js"
  }
}
