    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "2.1.0",
    "@opentelemetry/sdk-node": "0.205.0",
    "@opentelemetry/sdk-trace-base": "2.1.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.205.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.205.0",
    "opentelemetry-resource-detector-git": "0.30.0",
    "@opentelemetry/resource-detector-github": "0.31.0",
    "@opentelemetry/resource-detector-container": "0.7.5",
    "@opentelemetry/resource-detector-aws": "2.5.0",
    "@opentelemetry/resource-detector-gcp": "0.40.0",
    "xhr2": "0.2.1"
  },
  "scripts": {
    "start": "node discordgateway2http.js",
    "exec-start": "exec node discordgateway2http.js"
  }
